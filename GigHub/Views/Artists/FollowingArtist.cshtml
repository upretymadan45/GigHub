@model GigHub.ViewModels.ListArtistViewModel
@{
    ViewBag.Title = "FollowingArtist";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Artist I'm following</h2>
<ul class="voffset4">
    @foreach(var artist in Model.FollowingArtist)
    {
        <li>
            @artist.Name
        </li>
    }
</ul>

